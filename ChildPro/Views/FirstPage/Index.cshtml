<body>
    <!--------顶部------->
    <div class="page-first">
        <div class="head">
            <div class="container">
                <!--顶部具体内容-->
                <nav class="navbar">
                    <div class="navbar-header">
                        <a href="#" class="navbar-brand">
                            <img class="responsive-img" width="133" height="50" src="~/Image/FirstPage/cheng.png">
                        </a>
                    </div>
                    <div class="navbar-right">
                        <div class="link-btn1">
                            <a href="@Url.Action("Index","Login")" class="btn">登录</a>
                        </div>

                        <div class="link-btn2">
                            <a href="@Url.Action("Index","Register")" class="btn">注册</a>
                        </div>
                    </div>

                </nav>
            </div>

            <!--大轮播图-->
            <div id="myCarousel" class="carousel slide">
                <!-- 轮播（Carousel）指标 -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                </ol>
                <!-- 轮播（Carousel）项目 -->
                <div class="carousel-inner">
                    <div class="item active">
                        <img src="~/Image/FirstPage/轮播.jpg" alt="First slide">
                        <div class="carousel-caption">标题 1</div>
                    </div>
                    <div class="item">
                        <img src="~/Image/FirstPage/轮播.jpg" alt="Second slide">
                        <div class="carousel-caption">标题 2</div>
                    </div>
                    <div class="item">
                        <img src="~/Image/FirstPage/轮播.jpg" alt="Third slide">
                        <div class="carousel-caption">标题 3</div>
                    </div>
                </div>
                <!-- 轮播（Carousel）导航 -->
                <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>

            <!--表单-->
            <div class="container form">
                <form class="form-inline">
                    <div class="form-group">
                        <input type="text" class="form-control input01 input-phone" id="phone" placeholder="请输入您的手机号">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control input03" id="name" placeholder="孩子姓名">
                        <div class="message"></div>
                    </div>
                    @*<div class="form-together">
                        <div class="form-group">
                            <input type="text" class="form-control input03" id="name" placeholder="孩子姓名">
                            <div class="message"></div>
                        </div>
                        <div class="form-group">
                            <select class="form-control input04 select-form" value="就读年级">
                                <option value="" selected="" disabled="" style="display: none;">就读年级</option>
                                <option value="0">学龄前</option>
